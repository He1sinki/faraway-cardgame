{
	"name": "vue-express-template",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "concurrently \"node index\" \"cd website && npm run dev\"",
		"server": "node scripts/server.js",
		"bot": "node scripts/bot.js",
		"selfplay": "node scripts/selfplay.js",
		"selfplay:adv": "node scripts/selfplay.js --bots 8 --max-parallel 4 --episodes 100 --rotate --batch-size 25",
		"queue:consume": "node dataset/queue/queue_consumer.js",
		"selfplay+queue": "node scripts/selfplay_with_queue.js",
		"quality": "node scripts/quality_check.js",
		"train": "bash scripts/train.sh",
		"logs:compress": "node scripts/compress_logs.js",
		"logs:agg": "node scripts/aggregate_metrics.js",
		"dataset:build": "node scripts/build_dataset.js",
		"dataset:split": "node scripts/split_dataset.js",
		"episodes:compress": "node scripts/compress_episodes.js",
		"dataset:dedup": "node scripts/dedup_transitions.js",
		"dataset:normalize": "node scripts/normalize_dataset.js",
		"episodes:index": "node scripts/build_replay_index.js",
		"selfplay:collect": "BOTS=4 TARGET_EPISODES=20 node scripts/selfplay.js && npm run dataset:build && npm run dataset:dedup && npm run dataset:normalize && npm run dataset:split && npm run episodes:compress && npm run episodes:index",
		"ppo": "python rl/ppo_train.py",
		"test:encode": "node rl/test_encode_observation.js",
		"env:test": "node rl/test_env_basic.js",
		"export:npz": "node scripts/export_npz.js",
		"test:mask": "node rl/test_mask_consistency.js"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"axios": "^1.2.1",
		"body-parser": "^1.20.1",
		"concurrently": "^7.6.0",
		"cors": "^2.8.5",
		"express": "^4.18.2",
		"fs": "*",
		"http": "*",
		"pino": "^9.9.4",
		"socket.io": "^4.8.1",
		"socket.io-client": "^4.8.1"
	},
	"devDependencies": {
		"pino-pretty": "^11.3.0"
	}
}